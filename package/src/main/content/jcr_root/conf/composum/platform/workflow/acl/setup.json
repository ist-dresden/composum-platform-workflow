[
  {
    "path": "/var/composum/workflow",
    "jcr:primaryType": "sling:Folder",
    "acl": [
      {
        "principal": "composum-platform-users",
        "groupPath": "composum/platform",
        "acl": [
          {
            "allow": true,
            "privileges": "jcr:read",
            "restrictions": {
              "rep:glob": ""
            }
          }
        ]
      },
      {
        "principal": "composum-platform-administrators",
        "groupPath": "composum/platform",
        "acl": [
          {
            "allow": true,
            "privileges": "jcr:all"
          }
        ]
      }
    ]
  },
  {
    "path": "/var/composum/workflow/shared",
    "jcr:primaryType": "sling:Folder",
    "acl": [
      {
        "principal": "composum-platform-users",
        "acl": [
          {
            "allow": true,
            "privileges": "jcr:read"
          }
        ]
      }
    ]
  }
]
